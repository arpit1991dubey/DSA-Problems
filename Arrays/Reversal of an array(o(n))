#include <stdio.h>
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int k,n;
    int arr[n];
    cout<<"Enter length of array";
    cin>>n;
    cout<<"Enter the array elements";
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
  int start=0;
  int end=n-1;
  while(start<end)
  {
      swap(arr[start],arr[end]);
      start++;
      end--;
  }

    for(int i=0;i<n;i++)
{
    cout<<arr[i]<<" ";
}
    return 0;
}
